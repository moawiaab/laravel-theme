{
    "name": "moawiaab/laravel-theme",
    "description": "Install Laravel breeze themes for Vue",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Moawiaab\\LaravelTheme\\": "src/"
        }
    },
    "authors": [
        {
            "name": "moawia abudallah",
            "email": "moawiaab@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Moawiaab\\LaravelTheme\\LaravelThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require": {
        "spatie/laravel-medialibrary": "*",
        "spatie/laravel-permission"  : "*",
        "laravel/breeze": "*",
        "laravel/ui" : "*"
    }
}
